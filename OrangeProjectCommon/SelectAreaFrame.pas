end;

procedure TFrameSelectArea.FrameResize(Sender: TObject);
var
  AVisibleListBoxCount:Integer;
begin
  if FState<>nil then
  begin
    Self.lbProvience.Visible:=(Self.FState.ProvinceList.Count>1);
    Self.lbCity.Visible:=GlobalIsStateHasCity;
    Self.lbCounty.Visible:=GlobalIsStateHasArea and FIsNeedArea;

    AVisibleListBoxCount:=Ord(Self.lbProvience.Visible)
                          +Ord(Self.lbCity.Visible)
                          +Ord(Self.lbCounty.Visible);

    if AVisibleListBoxCount>0 then
    begin

      //平分三个ListBox
      Self.lbProvience.Width:=Self.Width/AVisibleListBoxCount;
      Self.lbCity.Width:=Self.Width/AVisibleListBoxCount;
      Self.lbCounty.Width:=Self.Width/AVisibleListBoxCount;

    end;
  end;
end;

procedure TFrameSelectArea.SetGPSLocation(AProvince, ACity, AArea: String);
begin

  FGPSProvince:=AProvince;
  FGPSCity:=ACity;
  FGPSArea:=AArea;

  Self.pnlDevide2.Visible:=(AProvince<>'');
  if Self.FIsNeedArea then
