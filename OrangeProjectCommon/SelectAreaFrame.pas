  AArea:TArea;
  AStringList:TStringList;
begin
  Self.AreaList.Clear(True);
  AStringList:=TStringList.Create;
  try
    AStringList.CommaText:=ACommaText;
    for I := 0 to AStringList.Count-1 do
    begin
      if AStringList[I]<>'' then
      begin
        GlobalIsStateHasArea:=True;
        AArea:=TArea.Create;
        AArea.name_zh:=AStringList[I];
        AArea.name_en:=AStringList[I];
        Self.AreaList.Add(AArea);
      end;
    end;
  finally
    FreeAndNil(AStringList);
  end;

end;

