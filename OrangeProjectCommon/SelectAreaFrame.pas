// 获取省
procedure TFrameSelectArea.GetAllProvience;
var
  provienceListBoxItem:TSkinListBoxItem;
  I: Integer;
  AProvince:TProvince;
begin

  Self.lbProvience.Prop.Items.BeginUpdate;
  try
    Self.lbProvience.Prop.Items.Clear(True);
    for I := 0 to Self.FState.ProvinceList.Count-1 do
    begin
      AProvince:=TProvince(Self.FState.ProvinceList[I]);
      provienceListBoxItem:=Self.lbProvience.Prop.Items.Add;
      if Self.FIsEnglish then
      begin
        provienceListBoxItem.Caption:=AProvince.name_en;
      end
      else
      begin
        provienceListBoxItem.Caption:=AProvince.name_zh;
      end;
      provienceListBoxItem.Data:=Self.FState.ProvinceList[I];
    end;
    Self.lbProvience.Prop.Items[0].Selected:=True;
  finally
    Self.lbProvience.Prop.Items.EndUpdate;
  end;

end;

// 获取城市
procedure TFrameSelectArea.GetAllCity;
var
  cityListBoxItem:TSkinListBoxItem;
  AProvince:TProvince;
  ACity:TCity;
  I,temp,J: Integer;
begin
  Self.lbCity.Prop.Items.BeginUpdate;
  try
    Self.lbCity.Prop.Items.Clear(True);
    if Self.lbProvience.Prop.SelectedItem<>nil then
    begin

      //获取当前选择的省份
      temp:=Self.lbProvience.Prop.SelectedItem.Index;

      AProvince:=TProvince(Self.FState.ProvinceList[temp]);
      for I := 0 to AProvince.CityList.Count-1 do
      begin
        ACity:=TCity(AProvince.CityList[I]);
        cityListBoxItem:=Self.lbCity.Prop.Items.Add;
        if Self.FIsEnglish then
        begin
          cityListBoxItem.Caption:=ACity.name_en;
        end
        else
        begin
          cityListBoxItem.Caption:=ACity.name_zh;
        end;
        cityListBoxItem.Data:=AProvince.CityList[I];
      end;

      //选中第一个城市
      Self.lbCity.Prop.Items[0].Selected:=True;

    end;
  finally
    Self.lbCity.Prop.Items.EndUpdate;
  end;

end;

