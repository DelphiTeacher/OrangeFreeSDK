function TState.ParseProvinceListFromJson(ASuperArray: ISuperArray): Boolean;
var
  I: Integer;
  AProvince:TProvince;
begin
  GlobalIsStateHasCity:=False;
  GlobalIsStateHasArea:=False;

  Self.ProvinceList.Clear(True);
  for I := 0 to ASuperArray.Length-1 do
  begin
    AProvince:=TProvince.Create;
    AProvince.name_zh:=ASuperArray.O[I].S['name'];
    AProvince.name_en:=ASuperArray.O[I].S['name'];
    Self.ProvinceList.Add(AProvince);
    AProvince.ParseCityListFromJson(ASuperArray.O[I].A['cities']);
  end;
end;

function TState.ParseProvinceListFromWeb(ASuperObject: ISuperObject): Boolean;
var
  I: Integer;
  J:Integer;
  AProvince:TProvince;
  AProvinceFID:Integer;
  ACityObject:ISuperObject;
  ACity:TCity;
  K: Integer;
  AAreaObject:ISuperObject;
  AArea:TArea;

  AIsCityFound:Boolean;
  AIsAreaFound:Boolean;

  ACityList:TBaseList;
  AAreaList:TBaseList;
begin
  GlobalIsStateHasCity:=False;
  GlobalIsStateHasArea:=False;

  ACityList:=TBaseList.Create(ooReference);
  AAreaList:=TBaseList.Create(ooReference);
  try
    Self.ProvinceList.Clear();


    //先取出来,遍历会比较快
    for J := 0 to ASuperObject.A['CityList'].Length-1 do
    begin
      ACityObject:=ASuperObject.A['CityList'].O[J];
      ACity:=TCity.Create;
      ACity.fid:=ACityObject.I['fid'];
      ACity.province_fid:=ACityObject.I['province_fid'];
      ACity.name_zh:=ACityObject.S['name_zh'];
      ACity.name_en:=ACityObject.S['name_en'];
      ACityList.Add(ACity);
    end;

    //先取出来,遍历会比较快
    for K := 0 to ASuperObject.A['AreaList'].Length-1 do
    begin
      AAreaObject:=ASuperObject.A['AreaList'].O[K];
      AArea:=TArea.Create;
      AArea.fid:=AAreaObject.I['fid'];
      AArea.province_fid:=AAreaObject.I['province_fid'];
      AArea.city_fid:=AAreaObject.I['city_fid'];
      AArea.name_zh:=AAreaObject.S['name_zh'];
      AArea.name_en:=AAreaObject.S['name_en'];
      AAreaList.Add(AArea);
    end;


    for I := 0 to ASuperObject.A['ProvinceList'].Length-1 do
    begin
      AProvince:=TProvince.Create;
      AProvince.fid:=ASuperObject.A['ProvinceList'].O[I].I['fid'];
      AProvince.name_zh:=ASuperObject.A['ProvinceList'].O[I].S['name_zh'];
      AProvince.name_en:=ASuperObject.A['ProvinceList'].O[I].S['name_en'];
      Self.ProvinceList.Add(AProvince);


      AIsCityFound:=False;
      for J := 0 to ACityList.Count-1 do
      begin
        if TCity(ACityList[J]).province_fid=AProvince.fid then
        begin

          ACity:=TCity(ACityList[J]);
          AIsCityFound:=True;
          GlobalIsStateHasCity:=True;
          //属于当前省的城市
          AProvince.CityList.Add(ACity);


          //属于当前城市的区域
          AIsAreaFound:=False;
          for K := 0 to AAreaList.Count-1 do
          begin
            if TArea(AAreaList[K]).city_fid=ACity.fid then
            begin
              AIsAreaFound:=True;
              GlobalIsStateHasArea:=True;
              ACity.AreaList.Add(AAreaList[K]);
            end
            else
            begin
              if AIsAreaFound then
              begin
                Break;
              end;
            end;
          end;

        end
        else
        begin
          if AIsCityFound then
          begin
            Break;
          end;
        end;
      end;


    end;




  finally
    FreeAndNil(ACityList);
    FreeAndNil(AAreaList);
  end;

end;

function TState.SaveAsCommaText: String;
var
  I: Integer;
begin
  Result:='';
  for I := 0 to Self.ProvinceList.Count-1 do
  begin
    Result:=Result+TProvince(Self.ProvinceList[I]).name_zh+',';
  end;

  //补齐
  for I := 0 to MaxCount-Self.ProvinceList.Count-1 do
  begin
    Result:=Result+',';
  end;

end;

end.











