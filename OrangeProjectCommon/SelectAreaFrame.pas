procedure TFrameSelectArea.ShowDefaultData;
begin
  // 设置默认选中数据
  if FIsDataLoaded then
  begin
    Self.GetAllProvience();

    Self.lbProvience.Prop.Items[FProvienceIndex].Selected:=True;
    FSelectedProvince:=Self.lbProvience.Prop.Items[FProvienceIndex].Caption;

    Self.lbCity.Prop.Items[FCityIndex].Selected:=True;
    FSelectedCity:= Self.lbCity.Prop.Items[FCityIndex].Caption;

    if FAreaIndex>=0 then
    begin
      Self.lbCounty.Prop.Items[FAreaIndex].Selected:=True;
      FSelectedArea:=Self.lbCounty.Prop.Items[FAreaIndex].Caption;
    end
    else
    begin
      FSelectedArea:='';
    end;

  end;

end;

procedure TFrameSelectArea.tteGetAreaExecute(Sender: TTimerTask);
var
  ASuperObject:ISuperObject;
begin
  Sender.TaskTag:=1;
  try
    //从接口返回数据
    Sender.TaskDesc:=//'';
          SimpleCallAPI(
                      'get_state_area',
                      nil,
                      FRestUrl,
                      ['state_name_en'],
                      [FRestStateNameEn],
                      '',
                      ''
                      );

    if Sender.TaskDesc<>'' then
    begin
        ASuperObject:=TSuperObject.Create(Sender.TaskDesc);
        if ASuperObject.I['Code']=200 then
        begin
          //在线程中就解析好,防止界面卡顿
          FState.ParseProvinceListFromWeb(ASuperObject.O['Data']);
          FIsDataLoaded:=True;
        end
        else
        begin
          //接口返回失败
        end;
        Sender.TaskTag:=0;
    end
    else
    begin
        Sender.TaskDesc:='数据获取失败';
    end;
  except
    on E:Exception do
    begin
      //异常
      Sender.TaskDesc:=E.Message;
    end;
  end;
end;

procedure TFrameSelectArea.tteGetAreaExecuteEnd(Sender: TTimerTask);
begin
  try
    if Sender.TaskTag=0 then
    begin

      Init(Self.FDelayLoadProvinceName,
                  Self.FDelayLoadCityName,
                  Self.FDelayLoadAreaName,
                  False,
                  FIsEnglish,
                  FIsNeedArea);

      FrameResize(nil);
    end
    else
    begin
      ShowMessageBoxFrame(Self,Sender.TaskDesc,'',TMsgDlgType.mtInformation,['确定'],nil);
    end;
  finally
    HideWaitingFrame;
  end;
end;

// 根据省市县各自段返回定位下标
procedure TFrameSelectArea.Init(AProvinceName,ACityName,AAreaName: String;
      AIsLoadDataFromConst:Boolean;
      AIsEnglish:Boolean;
      AIsNeedArea:Boolean);
var
  I,J,K:Integer;
  AProvince:TProvince;
  ACity:TCity;
  AArea:TArea;
begin

  //延迟加载,因为一开始地区并没有加载好
  FDelayLoadProvinceName:=AProvinceName;
  FDelayLoadCityName:=ACityName;
  FDelayLoadAreaName:=AAreaName;

  //是否是英文版的
  FIsEnglish:=AIsEnglish;
  //是否需要区域
  FIsNeedArea:=AIsNeedArea;


  //没有加载过就加载
  if AIsLoadDataFromConst and Not Self.FIsDataLoaded then
  begin
    //从常量中加载
    Self.LoadData;
  end;


  FProvienceIndex:=0;
  FCityIndex:=0;
  FAreaIndex:=-1;

  if (AProvinceName='') and (ACityName='') and (AAreaName='') then
  begin
      //初始定位的省市区
      FProvienceIndex:=0;
      FCityIndex:=0;
      FAreaIndex:=-1;

  end
  else
  begin

      for I := 0 to Self.FState.ProvinceList.Count-1 do
      begin
        AProvince:=TProvince(Self.FState.ProvinceList[I]);
        if (AProvince.name_zh=AProvinceName)
          or (ReplaceStr(AProvince.name_en,'/','-')=AProvinceName)
          or (AProvince.name_en=AProvinceName)
          //浙江,浙江省
          or (AProvince.name_zh+'省'=AProvinceName)
          or (AProvince.name_zh=AProvinceName+'省')
          //上海,上海市
          or (AProvince.name_zh+'市'=AProvinceName)
          or (AProvince.name_zh=AProvinceName+'市')
          then
        begin

            //找到省
            FProvienceIndex:=I;
            for J := 0 to AProvince.CityList.Count-1 do
            begin

                ACity:=TCity(AProvince.CityList[J]);

                if (ACity.name_zh=ACityName)
                  or (ACity.name_en=ACityName)
                  //金华,金华市
                  or (ACity.name_zh+'市'=ACityName)
                  or (ACity.name_zh=ACityName+'市')
                then
                begin
                    //找到市
                    FCityIndex:=J;

                    for K := 0 to ACity.AreaList.Count-1 do
                    begin
                      AArea:=TArea(ACity.AreaList[K]);
                      if (AArea.name_zh=AAreaName)
                        or (AArea.name_en=AAreaName)
                        //婺城,婺城区
                        or (AArea.name_zh+'区'=AAreaName)
                        or (AArea.name_zh=AAreaName+'区')
                      then
                      begin
                        //找到区
                        FAreaIndex:=K;

                        Break;
                      end;
                    end;

                    //找到市
                    Break;

                end;
            end;

            //找到省
            Break;

        end;

      end;
  end;


  ShowDefaultData();

  FrameResize(nil);
end;



