<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hifond.cardlibrary"
    android:versionCode="308"
    android:versionName="card_aar_3.0.8" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="28" />

    <!-- &lt;!&ndash; 只有申明了这个权限的APP才有权限接受我发出的读卡数据返回广播 &ndash;&gt; -->
    <!-- <permission android:name = "${applicationId}.RECV_CARD_DATA_PERMISSION" -->
    <!-- android:protectionLevel="normal"/> -->
    <!-- 允许应用写入外部存储 -->
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <!-- 允许应用读取外部存储 -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:label="@string/app_name"
        android:supportsRtl="true" >


        <service
            android:name="com.ysf.card.server.HiUpdateHexServer"
            android:enabled="true"
            android:exported="true" >
            <intent-filter>
                <action android:name="com.hifond.jso.hexServer.AIDLSERVER" />
            </intent-filter>
        </service>
        <service
            android:name="com.ysf.card.server.HiSoCardServer"
            android:enabled="true"
            android:exported="true" />
        <service
            android:name="com.ysf.card.server.HiNewCardServer"
            android:enabled="true"
            android:exported="true" />
        <service
            android:name="com.ysf.card.server.HiCardServer"
            android:enabled="false"
            android:exported="false" />
        <service
            android:name="com.ysf.card.server.HiLoCardServer"
            android:enabled="true"
            android:exported="true" />
        <service
            android:name="com.ysf.card.server.HiAIDLCardServer"
            android:enabled="true"
            android:exported="true" >
            <intent-filter>
                <action android:name="com.hifond.jso.cardServer.AIDLSERVER" />
            </intent-filter>
        </service>
        <service
            android:name="com.ysf.card.server.HiMaibobServer"
            android:enabled="true" >
            <intent-filter>
                <action android:name="com.maibo.unlock" />
                <action android:name="com.maibo.lock" />
                <action android:name="com.maibo.measure" />
                <action android:name="com.maibo.stop" />
                <action android:name="com.maibo.conn" />
                <action android:name="com.l06b.start" />
                <action android:name="com.l06b.cancel" />
                <action android:name="com.card.start" />
                <action android:name="com.card.stop" />
                <action android:name="com.card.bee" />
                <action android:name="com.card.check" />
            </intent-filter>
        </service>
    </application>

</manifest>